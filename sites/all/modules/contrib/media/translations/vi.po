msgid ""
"Cannot update field %id (%field_name) because it does not use the "
"field_sql_storage storage type."
msgstr ""
"Không cập nhật được trường %id (%field_name) vì nó "
"không dùng kiểu lưu trữ field_sql_storage."
msgid "File saved"
msgstr "Đã lưu tệp"
